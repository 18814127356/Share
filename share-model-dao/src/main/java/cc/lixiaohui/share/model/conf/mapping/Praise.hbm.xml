<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="cc.lixiaohui.share.model.bean">
	<class name="Praise" table="praise">
		<id name="id" type="int">
			<column name="id" />
			<generator class="native" />
		</id>
		<property name="praiseTime" type="java.sql.Timestamp">
			<column name="praise_time" />
		</property>
		<!-- many to one -->
		<many-to-one name="user" class="cc.lixiaohui.share.model.bean.User"
			fetch="join">
			<column name="user_id" />
		</many-to-one>
		<many-to-one name="share" class="cc.lixiaohui.share.model.bean.Share"
			fetch="join">
			<column name="share_id" />
		</many-to-one>
	</class>
</hibernate-mapping>
