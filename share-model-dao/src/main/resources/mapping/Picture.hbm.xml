<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="cc.lixiaohui.share.model.bean">
	<class name="Picture" table="picture">
		<id name="id" type="int" column="id">
			<generator class="native" />
		</id>
		<property name="path" type="java.lang.String" column="path" update="false"/>
		<property name="suffix" type="java.lang.String" column="suffix" update="false"/>
		<property name="uploadUserId" type="int" column="upload_user_id" update="false"></property>
		<property name="uploadTime" type="java.sql.Timestamp" column="upload_time" update="false"/>
	</class>
</hibernate-mapping>
